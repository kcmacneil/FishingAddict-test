# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'FishingAddict' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

# Pods for FishingAddict
  #Pods for Firebase Database
  pod 'Firebase/Database'
  #Pods for Firebase Storage
  pod 'Firebase/Storage'
  #Pods for Firebase Authentication
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Google'
  pod 'FirebaseUI/Facebook'
  
  #Pod for KingFisher used to display images
  pod 'Kingfisher', '~> 4.0'
  target 'FishingAddictTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FishingAddictUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
